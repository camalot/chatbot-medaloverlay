{
	"output_file": "settings.json",
	"Command": {
		"type": "textbox",
		"value": "!clip",
		"label": "Clip Command",
		"tooltip": "The command used to create the clip",
		"group": "General"
	},
	"OnlyTriggerOffCommand": {
		"type": "checkbox",
		"label": "Only Trigger off the Command",
		"value": false,
		"tooltip": "If enabled, videos triggered from pressing the hotkey will now playback on stream.",
		"group": "General"
	},

	"RequiredTriggerCount": {
		"type": "slider",
		"label": "Required Commands to Trigger",
		"value": 1,
		"min": 1,
		"max": 100,
		"ticks": 1,
		"tooltip": "Number of commands required to trigger the clip",
		"group": "General"
	},
	"TriggerCooldown": {
		"type": "slider",
		"label": "Trigger Cooldown (seconds)",
		"value": 30,
		"min": 1,
		"max": 120,
		"ticks": 1,
		"tooltip": "The time frame in which commands count to trigger a clip",
		"group": "General"
	},
	"Cooldown": {
		"type": "slider",
		"label": "Cooldown (seconds)",
		"value": 60,
		"min": 1,
		"max": 600,
		"ticks": 1,
		"tooltip": "Cooldown in seconds after triggered.",
		"group": "General"
	},
	"Permission": {
		"type": "dropdown",
